{
    "name": "bpato/tareados",
    "type": "prestashop-module",
    "authors": [
        {
            "name": "Brais Pato",
            "email": "braispato@hotmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bpato\\Tareatres\\": [
                "src/"
            ]
        },
        "classmap": [
            "tareatres.php",
            "controllers/"
        ],
        "exclude-from-classmap": []
    },
    "require": {
        "php": ">=7.1.0"
    },
    "config": {
        "preferred-install": "dist",
        "prepend-autoloader": false,
        "optimize-autoloader": true
    }
}
